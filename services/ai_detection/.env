# MediaMTX
MEDIAMTX_URL=http://mediamtx:9997
MEDIAMTX_WEBRTC_URL=http://mediamtx:8889
USE_WEBRTC=true

# Frame Processing
AI_FPS=3
RTSP_TIMEOUT_MS=5000
RTSP_RETRY_DELAY=5

# Motion Detection
MOTION_THRESHOLD=0.03
MOG2_VAR_THRESHOLD=16
MOG2_HISTORY=500

# Vehicle Detection
VEHICLE_CONFIDENCE=0.5
VEHICLE_MODEL=models/vehicle_yolov8n.pt

# Tracking
TRACKER_IOU_THRESHOLD=0.3
TRACKER_TIMEOUT=5

# Quality Scoring
QUALITY_WEIGHT_BLUR=0.35
QUALITY_WEIGHT_ANGLE=0.30
QUALITY_WEIGHT_CONTRAST=0.20
QUALITY_WEIGHT_SIZE=0.15
MIN_QUALITY_SCORE=50

# Plate Detection (FINE-TUNED MODEL for License Plates)
PLATE_CONFIDENCE=0.6
PLATE_MODEL=models/plate_yolov8n.pt

# OCR
OCR_MODEL=cct-xs-v1-global-model

# Consensus
MIN_READINGS=3
MAX_READINGS=5
CONSENSUS_THRESHOLD=0.6
SIMILARITY_THRESHOLD=0.8
MIN_CONFIDENCE=0.75

# Deduplication
REDIS_HOST=redis
REDIS_PORT=6379
DEDUP_TTL=300

# Backend
BACKEND_URL=http://backend:8000
RABBITMQ_HOST=rabbitmq
RABBITMQ_PORT=5672
RABBITMQ_USER=guest
RABBITMQ_PASS=guest

# API
API_PORT=5000
